{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "id-length": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "no-shadow": 0,
    "func-names": 0,
    "object-shorthand": [2, "methods"],
    "max-len": [2, 120, 2],
    "space-before-function-paren": 0,
    "no-loop-func": 1
  }
}
